{
  "name": "hydrogen-sanity",
  "version": "5.0.0-next.1",
  "description": "Sanity.io toolkit for Hydrogen",
  "keywords": [
    "sanity",
    "sanity.io",
    "shopify",
    "hydrogen",
    "remix",
    "live",
    "preview",
    "react-router"
  ],
  "homepage": "https://github.com/sanity-io/hydrogen-sanity#readme",
  "bugs": {
    "url": "https://github.com/sanity-io/hydrogen-sanity/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/hydrogen-sanity.git"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./preview/route": {
      "source": "./src/preview/route.ts",
      "import": "./dist/preview/route.js",
      "default": "./dist/preview/route.js"
    },
    "./visual-editing": {
      "source": "./src/visual-editing/index.ts",
      "import": "./dist/visual-editing/index.js",
      "default": "./dist/visual-editing/index.js"
    },
    "./vite": {
      "source": "./src/vite/index.ts",
      "import": "./dist/vite/index.js",
      "default": "./dist/vite/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "preview/route": [
        "./dist/preview/route.d.ts"
      ],
      "visual-editing": [
        "./dist/visual-editing/index.d.ts"
      ],
      "vite": [
        "./dist/vite/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "pkg-utils build --strict --clean --check",
    "release": "semantic-release",
    "release:debug": "semantic-release --dry-run --debug",
    "release:no-ci": "GITHUB_TOKEN=`gh auth token` semantic-release --no-ci --debug --dryRun",
    "lint": "eslint --cache --config eslint.config.js .",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc",
    "watch": "pkg-utils watch --strict"
  },
  "dependencies": {
    "@sanity/client": "^7.3.0",
    "@sanity/preview-url-secret": "^2.1.11",
    "@sanity/react-loader": "^1.11.10",
    "@sanity/visual-editing": "^2.13.22",
    "react-compiler-runtime": "19.1.0-rc.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.27.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@sanity/pkg-utils": "^7.2.2",
    "@sanity/semantic-release-preset": "^5.0.0",
    "@shopify/hydrogen": "~2025.5.0",
    "@types/react": "^18.3.22",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "babel-plugin-react-compiler": "19.1.0-rc.2",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-config-sanity": "^7.1.4",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "7.6.0",
    "semantic-release": "^24.2.5",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.1.4"
  },
  "peerDependencies": {
    "@sanity/client": "^7",
    "@shopify/hydrogen": "~2025.5.0",
    "react": "^18.2.0",
    "react-router": "7.6.0",
    "vite": "^5.1.0 || ^6.2.1"
  },
  "engines": {
    "node": ">=20"
  }
}
