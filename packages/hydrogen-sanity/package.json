{
  "name": "hydrogen-sanity",
  "version": "6.0.0",
  "description": "Sanity.io toolkit for Hydrogen",
  "keywords": [
    "sanity",
    "sanity.io",
    "shopify",
    "hydrogen",
    "live",
    "preview",
    "react-router"
  ],
  "homepage": "https://github.com/sanity-io/hydrogen-sanity#readme",
  "bugs": {
    "url": "https://github.com/sanity-io/hydrogen-sanity/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/hydrogen-sanity.git",
    "directory": "packages/hydrogen-sanity"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "provenance": true,
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "default": "./dist/index.js"
      },
      "./preview": {
        "import": "./dist/preview/index.js",
        "default": "./dist/preview/index.js"
      },
      "./preview/route": {
        "import": "./dist/preview/route.js",
        "default": "./dist/preview/route.js"
      },
      "./preview/session": {
        "import": "./dist/preview/session.js",
        "default": "./dist/preview/session.js"
      },
      "./visual-editing": {
        "import": "./dist/visual-editing/index.js",
        "default": "./dist/visual-editing/index.js"
      },
      "./vite": {
        "import": "./dist/vite/index.js",
        "default": "./dist/vite/index.js"
      },
      "./package.json": "./package.json"
    }
  },
  "browserslist": "extends @sanity/browserslist-config",
  "type": "module",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./preview": {
      "source": "./src/preview/index.ts",
      "import": "./dist/preview/index.js",
      "default": "./dist/preview/index.js"
    },
    "./preview/route": {
      "source": "./src/preview/route.ts",
      "import": "./dist/preview/route.js",
      "default": "./dist/preview/route.js"
    },
    "./preview/session": {
      "source": "./src/preview/session.ts",
      "import": "./dist/preview/session.js",
      "default": "./dist/preview/session.js"
    },
    "./visual-editing": {
      "source": "./src/visual-editing/index.ts",
      "import": "./dist/visual-editing/index.js",
      "default": "./dist/visual-editing/index.js"
    },
    "./vite": {
      "source": "./src/vite/index.ts",
      "import": "./dist/vite/index.js",
      "default": "./dist/vite/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "preview": ["./dist/preview/index.d.ts"],
      "preview/route": ["./dist/preview/route.d.ts"],
      "preview/session": ["./dist/preview/session.d.ts"],
      "visual-editing": ["./dist/visual-editing/index.d.ts"],
      "vite": ["./dist/vite/index.d.ts"]
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "pkg-utils build --strict --clean --check",
    "release": "semantic-release",
    "release:debug": "semantic-release --dry-run --debug",
    "release:no-ci": "GITHUB_TOKEN=`gh auth token` semantic-release --no-ci --debug --dryRun",
    "lint": "eslint --cache --config eslint.config.js .",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc",
    "watch": "pkg-utils watch --strict"
  },
  "dependencies": {
    "@sanity/comlink": "^4.0.1",
    "@sanity/core-loader": "^2.0.5",
    "@sanity/image-url": "^2.0.2",
    "@sanity/presentation-comlink": "^2.0.1",
    "@sanity/preview-url-secret": "^4.0.2",
    "@sanity/react-loader": "^2.0.5",
    "@sanity/visual-editing": "^4.0.3",
    "use-effect-event": "^2.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@sanity/client": "^7.14.0",
    "@sanity/pkg-utils": "^10.3.0",
    "@sanity/semantic-release-preset": "^6.0.0",
    "@shopify/hydrogen": "~2025.7.2",
    "@testing-library/react": "^16.3.1",
    "@types/react": "^18.3.24",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-sanity": "^7.1.4",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "groq": "^5.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "7.12.0",
    "semantic-release": "^25.0.2",
    "typescript": "^5.9.3",
    "vite": "^6.3.5",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "@sanity/client": "^7",
    "@shopify/hydrogen": "~2025.5.0 || ~2025.7.0",
    "react": "^18.2.0 || ^19.0.0",
    "react-router": "^7.6.0",
    "vite": "^5.1.0 || ^6.2.1"
  },
  "engines": {
    "node": ">=20.19 <22 || >=22.12"
  }
}
