{
  "name": "@repo/sanity-config",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./schema.json": "./schema.json",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "pkg-utils build --strict --clean --check",
    "typecheck": "tsc",
    "lint": "eslint --cache --config eslint.config.mjs .",
    "watch": "pkg-utils watch --strict",
    "extract": "sanity schema extract",
    "extract:watch": "turbo watch --filter=\"@repo/sanity-config\" extract"
  },
  "dependencies": {
    "@sanity/asset-utils": "^2.3.0",
    "@sanity/color-input": "^6.0.4",
    "@sanity/icons": "^3.7.4",
    "@sanity/ui": "^3.1.11",
    "@sanity/vision": "^5.9.0",
    "lodash.get": "^4.4.2",
    "pluralize-esm": "^9.0.5",
    "sanity-plugin-hotspot-array": "^3.0.2",
    "sanity-plugin-media": "^4.1.1",
    "slug": "^11.0.1"
  },
  "devDependencies": {
    "@portabletext/types": "^4.0.1",
    "@sanity/eslint-config-studio": "^6.0.0",
    "@sanity/pkg-utils": "^10.4.4",
    "@types/lodash.get": "^4.4.9",
    "@types/react": "^19.2",
    "@types/slug": "^5.0.9",
    "eslint": "^9.39.2",
    "react": "^19.2",
    "react-dom": "^19.2",
    "sanity": "^5.9.0",
    "styled-components": "^6.3.9",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "react": "^19.2",
    "react-dom": "^19.2",
    "sanity": "^5",
    "styled-components": "^6.1"
  }
}
