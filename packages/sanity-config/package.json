{
  "name": "@repo/sanity-config",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./schema.json": "./schema.json",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "pkg-utils build --strict --clean --check",
    "typecheck": "tsc",
    "lint": "eslint --cache --config eslint.config.mjs .",
    "watch": "pkg-utils watch --strict",
    "extract": "sanity schema extract",
    "extract:watch": "turbo watch --filter=\"@repo/sanity-config\" extract"
  },
  "dependencies": {
    "@sanity/asset-utils": "^1.3.2",
    "@sanity/color-input": "^3.1.2",
    "@sanity/icons": "^2.11.8",
    "@sanity/ui": "^2.16.2",
    "@sanity/vision": "^3.94.2",
    "lodash.get": "^4.4.2",
    "pluralize-esm": "^9.0.5",
    "react-compiler-runtime": "19.1.0-rc.2",
    "sanity-plugin-hotspot-array": "^1.0.1",
    "sanity-plugin-media": "^2.4.2",
    "slug": "^8.2.3"
  },
  "devDependencies": {
    "@portabletext/types": "^2.0.13",
    "@sanity/eslint-config-studio": "^5.0.2",
    "@sanity/pkg-utils": "^7.8.5",
    "@types/lodash.get": "^4.4.9",
    "@types/react": "^18.3.23",
    "@types/slug": "^5.0.9",
    "babel-plugin-react-compiler": "19.1.0-rc.2",
    "eslint": "^9.29.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sanity": "latest",
    "styled-components": "^6.1.19",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "react": "^18.3",
    "react-dom": "^18.3",
    "sanity": "^3.23",
    "styled-components": "^6.1"
  }
}
