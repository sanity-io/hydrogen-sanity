{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "outputs": ["dist", ".sanity"]
    },
    "lint": {
      "outputs": [".eslintcache"]
    },
    "typecheck": {
      "outputs": [".tsbuildinfo"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
