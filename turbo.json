{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "outputs": ["dist"]
    },
    "lint": {
      "cache": false,
      "outputs": [".eslintcache"]
    },
    "typecheck": {
      "cache": false,
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
