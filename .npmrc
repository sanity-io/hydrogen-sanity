# Suppress the progress bar
# https://docs.npmjs.com/cli/v9/using-npm/config#progress
progress=false

# Adjust log level
# https://docs.npmjs.com/cli/v9/using-npm/config#loglevel
loglevel="warn"

# Disable audit reports
# https://docs.npmjs.com/cli/v9/using-npm/config#audit
audit=false

# Disable funding message
# https://docs.npmjs.com/cli/v9/using-npm/config#fund
fund=false